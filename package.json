{
  "name": "trillo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:scss": "node-sass scss/main.scss css/styles.css -w",
    "dev-server": "live-server",
    "start": "npm-run-all --parallel dev-server watch:scss",
    "compile:scss": "node-sass scss/main.scss css/styles.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/styles.comp.css -o css/styles.prefix.css",
    "compress:css": "node-sass --output-style compressed css/styles.prefix.css css/styles.css",
    "build:css": "npm-run-all compile:scss prefix:css compress:css"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
